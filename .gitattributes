# lfs
*.pdf       diff=lfs filter=lfs merge=lfs -text

# better diff
*.c         diff=cpp
*.c++       diff=cpp
*.cc        diff=cpp
*.cpp       diff=cpp
*.cs        diff=csharp
*.css       diff=css
*.el        diff=lisp
*.ex        diff=elixir
*.exs       diff=elixir
*.go        diff=golang
*.h         diff=cpp
*.h++       diff=cpp
*.hh        diff=cpp
*.hpp       diff=cpp
*.html      diff=html
*.lisp      diff=lisp
*.m         diff=objc
*.markdown  diff=markdown
*.md        diff=markdown
*.mm        diff=objc
*.php       diff=php
*.pl        diff=perl
*.py        diff=python
*.rake      diff=ruby
*.rb        diff=ruby
*.rs        diff=rust
*.xhtml     diff=html
